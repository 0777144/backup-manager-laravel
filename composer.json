{
  "name": "backup-manager/laravel",
  "description": "Database backup manager seamlessly integrated with Laravel 5.2 (minimum) with user-definable procedures and support for S3, Dropbox, FTP, SFTP, and more.",
  "authors": [
    {
      "name": "Shawn McCool",
      "email": "shawn@heybigname.com",
      "homepage": "http://heybigname.com/"
    },
    {
      "name": "Mitchell van Wijngaarden",
      "email": "mitchell@kooding.nl",
      "homepage": "http://heybigname.com/"
    }
  ],
  "require": {
    "backup-manager/backup-manager": "2.0.x-dev",
    "php": ">=5.6",
    "symfony/process": "~3.0",
    "illuminate/support": "~5.2",
    "illuminate/container": "~5.2",
    "illuminate/console": "~5.2"
  },
  "require-dev": {
    "satooshi/php-coveralls": "~1.0"
  },
  "config": {
    "bin-dir": "bin"
  },
  "autoload": {
    "psr-4": {
      "BackupManager\\Laravel\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-2.0": "2.0-dev"
    }
  },
  "license": "MIT"
